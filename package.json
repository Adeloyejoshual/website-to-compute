{
  "name": "minimart-marketplace",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon src/backend/index.js",
    "dev:frontend": "vite --config vite.config.js",
    "build": "cd src/frontend && npm install && npm run build",
    "start": "node src/backend/index.js"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.0",
    "nodemon": "^3.0.2",
    "vite": "^4.6.14"
  },
  "engines": {
    "node": "20.x"
  }
}